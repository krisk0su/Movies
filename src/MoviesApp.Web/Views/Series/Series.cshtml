@model RenderViewModel
@{
    ViewData["Title"] = "Series";
}
<div class="container-fluid" style="background-color: #393e46; ">
    <!-- Heading -->
    <div class="row">
        <div class="col">
            <div class="mb-5">
                <h1 class="text-center text-uppercase font-weight-bold" style="color: #00adb5">
                    WATCH Series ONLINE
                </h1>
                <hr class="hr" />
            </div>
        </div>
    </div>
    <!-- Cards -->
    @await Component.InvokeAsync("SeriesRender", 
        new
        {
           model = this.Model
        });
</div>


